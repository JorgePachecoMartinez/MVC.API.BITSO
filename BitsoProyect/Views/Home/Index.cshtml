@{
    Layout = "";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
    <div class="pt-5 container">
        <div>
            <h1 class="text-center">Compra, vende y cambia</h1>
        </div>
        <div class="py-5 row">
            <div class="p-2 col-12 col-md-6">
                <button type="button" class="btn btn-primary btn-block" onclick="location.href='@Url.Action("Index", "Bitcoin")'">
                    <img class="p-2 w-25" src="data:image/svg+xml;utf8;base64," />
                    Bitcoin
                </button>
            </div>
            <div class="p-2 col-12 col-md-6">
                <button type="button" class="btn btn-primary btn-block"  onclick="location.href='@Url.Action("Index", "Ether")'">
                    <img class="p-2 w-25" src="data:image/svg+xml;utf8;base64," />
                    Ether
                </button>
            </div>
        </div>
        <h1 class="text-center">en México</h1>
    </div>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>